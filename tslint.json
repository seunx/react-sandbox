{
    "defaultSeverity": "error",
    "extends": [
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "member-access": false,
        "orderd-imports": false,
        "quotemark": false,
        "no-console": false,
        "semicolon": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": [],
    "linterOptions":{
        "exclude":[
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    }
}